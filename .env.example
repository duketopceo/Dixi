# GCP Configuration (for scaling)
GCP_PROJECT_ID=
GCP_REGION=us-central1
GCP_SERVICE_ACCOUNT_KEY=

# OpenGL/Vulkan
RENDER_API=webgl
ENABLE_GPU_ACCELERATION=true
